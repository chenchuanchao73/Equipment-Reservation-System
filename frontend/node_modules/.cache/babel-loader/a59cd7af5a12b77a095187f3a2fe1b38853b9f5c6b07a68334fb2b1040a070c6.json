{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport MobileNav from './MobileNav.vue';\nexport default {\n  name: 'AppHeader',\n  components: {\n    MobileNav\n  },\n  computed: {\n    ...mapGetters(['isLoggedIn', 'getLanguage']),\n    activeIndex() {\n      return this.$route.path;\n    },\n    currentLanguage() {\n      return this.getLanguage;\n    }\n  },\n  methods: {\n    ...mapActions(['setLanguage']),\n    goToAdmin() {\n      this.$router.push('/admin/dashboard');\n    },\n    goToLogin() {\n      this.$router.push('/admin/login');\n    },\n    handleLanguageChange(lang) {\n      this.setLanguage(lang);\n      this.$i18n.locale = lang;\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","MobileNav","name","components","computed","activeIndex","$route","path","currentLanguage","getLanguage","methods","goToAdmin","$router","push","goToLogin","handleLanguageChange","lang","setLanguage","$i18n","locale"],"sources":["src/components/layout/AppHeader.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 桌面端导航 -->\n    <div class=\"app-header desktop-nav\">\n      <el-menu\n        :default-active=\"activeIndex\"\n        class=\"el-menu-demo\"\n        mode=\"horizontal\"\n        router\n        background-color=\"#409EFF\"\n        text-color=\"#fff\"\n        active-text-color=\"#ffd04b\"\n      >\n        <div class=\"logo-container\">\n          <router-link to=\"/\" class=\"logo-link\">\n            <img src=\"@/assets/images/htnia-logo.png\" alt=\"HTNIA Logo\" class=\"logo-image\" />\n            <span class=\"logo-text\">{{ $t('common.appName') }}</span>\n          </router-link>\n        </div>\n\n        <el-menu-item index=\"/\">{{ $t('nav.home') }}</el-menu-item>\n        <el-menu-item index=\"/equipment\">{{ $t('nav.equipment') }}</el-menu-item>\n        <el-menu-item index=\"/reservation/query\">{{ $t('nav.query') }}</el-menu-item>\n\n        <div class=\"right-menu\">\n          <div class=\"language-switcher\">\n            <button\n              class=\"lang-btn\"\n              :class=\"{ active: currentLanguage === 'zh-CN' }\"\n              @click=\"handleLanguageChange('zh-CN')\"\n            >\n              中文\n            </button>\n            <span class=\"divider\">|</span>\n            <button\n              class=\"lang-btn\"\n              :class=\"{ active: currentLanguage === 'en' }\"\n              @click=\"handleLanguageChange('en')\"\n            >\n              English\n            </button>\n          </div>\n\n        <el-button\n          v-if=\"isLoggedIn\"\n          type=\"text\"\n          class=\"admin-button\"\n          @click=\"goToAdmin\"\n          style=\"min-width: 100px;\"\n        >\n          {{ $t('nav.admin') }}\n        </el-button>\n        <el-button\n          v-else\n          type=\"text\"\n          class=\"login-button\"\n          @click=\"goToLogin\"\n          style=\"min-width: 100px;\"\n        >\n          {{ $t('nav.login') }}\n        </el-button>\n      </div>\n    </el-menu>\n    </div>\n\n    <!-- 移动端导航 -->\n    <mobile-nav class=\"mobile-nav\" />\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex'\nimport MobileNav from './MobileNav.vue'\n\nexport default {\n  name: 'AppHeader',\n\n  components: {\n    MobileNav\n  },\n\n  computed: {\n    ...mapGetters(['isLoggedIn', 'getLanguage']),\n\n    activeIndex() {\n      return this.$route.path\n    },\n\n    currentLanguage() {\n      return this.getLanguage\n    }\n  },\n\n  methods: {\n    ...mapActions(['setLanguage']),\n\n    goToAdmin() {\n      this.$router.push('/admin/dashboard')\n    },\n\n    goToLogin() {\n      this.$router.push('/admin/login')\n    },\n\n    handleLanguageChange(lang) {\n      this.setLanguage(lang)\n      this.$i18n.locale = lang\n    }\n  }\n}\n</script>\n\n<style scoped>\n.app-header {\n  width: 100vw !important;\n  margin: 0 !important;\n  padding: 0 !important;\n}\n\n.el-menu {\n  display: flex;\n  align-items: center;\n  padding: 0 !important;\n  width: 100vw !important;\n  margin: 0 !important;\n  border-radius: 0 !important;\n  border: none !important;\n}\n\n.logo-container {\n  margin-right: 20px;\n  margin-left: 20px;\n}\n\n.logo-link {\n  display: flex;\n  align-items: center;\n  text-decoration: none;\n  color: #fff;\n}\n\n.logo-image {\n  height: 40px;\n  margin-right: 10px;\n}\n\n.logo-text {\n  font-size: 18px;\n  font-weight: bold;\n}\n\n.right-menu {\n  margin-left: auto;\n  display: flex;\n  align-items: center;\n}\n\n.language-switcher {\n  display: flex;\n  align-items: center;\n  margin-right: 20px;\n}\n\n.lang-btn {\n  background: none;\n  border: none;\n  cursor: pointer;\n  padding: 5px 8px;\n  color: #fff;\n  font-size: 14px;\n}\n\n.lang-btn.active {\n  color: #ffd04b;\n  font-weight: bold;\n}\n\n.divider {\n  color: #fff;\n  margin: 0 5px;\n  opacity: 0.7;\n}\n\n.admin-button,\n.login-button {\n  color: #fff;\n  margin-left: 10px;\n}\n\n@media (max-width: 768px) {\n  .desktop-nav {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AAuEA,SAAAA,UAAA,EAAAC,UAAA;AACA,OAAAC,SAAA;AAEA;EACAC,IAAA;EAEAC,UAAA;IACAF;EACA;EAEAG,QAAA;IACA,GAAAL,UAAA;IAEAM,YAAA;MACA,YAAAC,MAAA,CAAAC,IAAA;IACA;IAEAC,gBAAA;MACA,YAAAC,WAAA;IACA;EACA;EAEAC,OAAA;IACA,GAAAV,UAAA;IAEAW,UAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IAEAC,UAAA;MACA,KAAAF,OAAA,CAAAC,IAAA;IACA;IAEAE,qBAAAC,IAAA;MACA,KAAAC,WAAA,CAAAD,IAAA;MACA,KAAAE,KAAA,CAAAC,MAAA,GAAAH,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}